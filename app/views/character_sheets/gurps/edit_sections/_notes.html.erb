<fieldset id='notes' class='form-group row'>
  <legend><%= t('.notes') %></legend>
    <div class='col-12'>

      <% 0.upto(8) do |i| %>
        <div class='row'>
          <div class='col-12'>
            <div class='form-group'>
              <%= f.text_field "characternotes_#{i}".to_sym, class: 'form-control', placeholder: t('.note') %>
            </div>
          </div>
        </div>
      <% end %>

    </div>
</fieldset>
